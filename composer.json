{
    "name": "chaplean/csv-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Parse CSV files",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "http://www.chaplean.coop"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chaplean\\Bundle\\CsvBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.chaplean.coop"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "symfony/config": "^3.0 || ^4.0",
        "symfony/console": "^3.0 || ^4.0",
        "symfony/dependency-injection": " ^3.0 || ^4.0",
        "symfony/translation": "^3.0 || ^4.0",
        "symfony/yaml": "^3.0 || ^4.0"
    },
    "require-dev": {
        "chaplean/codeship-scripts": "^1.0",
        "chaplean/coding-standard": "^1.1",
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "^7.0",
        "symfony/http-kernel": "^3.0 || ^4.0",
        "symfony/var-dumper": "^3.0 || ^4.0",
        "symfony/phpunit-bridge": "^4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var"
    }
}
